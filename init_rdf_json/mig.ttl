{
bind (?param? as ?param)  
 ?param rdf:type mig:msdash .
 ?ch1 js:parentJsonId ?param .
 ?ch1  js:hasJsonObjectKey 'model' .
 ?ch1 js:defaultPowerBIDataSourceVersion ?defaultPowerBIDataSourceVersion .
 ?ch2 js:parentJsonId ?ch1 .
